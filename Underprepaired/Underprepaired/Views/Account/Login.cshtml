@model Underprepaired.Models.ViewModels.LoginViewModel

@{ 
    ViewData["Title"] = "Login";
}

<h2>Login</h2>

<form method="post">

    <label asp-for="Email"></label>
    <input asp-for="Email" />
        
    <label asp-for="Password"></label>
    <input asp-for="Password" />

    <button type="submit">Login!</button>
</form>
